{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "amd": true,
    "browser": true
  },
  "rules": {
    "no-console": ["warn", {
      "allow": [
        "warn",
        "error"
      ]
    }],
    "import/no-unresolved": "off"
  }
}